<?xml version="1.0" encoding="utf-8"?>
<FullDefaultLayoutTemplate 
    xmlns="http://schemas.microsoft.com/Start/2014/FullDefaultLayout"
    xmlns:start="http://schemas.microsoft.com/Start/2014/StartLayout"
    Version="1">
    <StartLayoutCollection>
        <!-- 6 cell wide Desktop layout with targeted content tiles and Office 365 group and no skype -->
        <StartLayout
            GroupCellWidth="6"
            TargetedContentTilesEnabled="true"
            OfficeSKU="DesktopBridgeSubscription"
            Region="CN">
            <start:Group
                LocalizedNameResourceTag="TileGrid_Office365Group">
                <start:Tile
                    AppUserModelID="Microsoft.MicrosoftOfficeHub_8wekyb3d8bbwe!Microsoft.MicrosoftOfficeHub"
                    Size="2x2"
                    Row="0"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="Microsoft.Office.Desktop_8wekyb3d8bbwe!Outlook"
                    Size="2x2"
                    Row="0"
                    Column="2"/>
                <start:Tile
                    AppUserModelID="Microsoft.Office.Desktop_8wekyb3d8bbwe!Word"
                    Size="2x2"
                    Row="0"
                    Column="4"/>
                <start:Tile
                    AppUserModelID="Microsoft.Office.Desktop_8wekyb3d8bbwe!Excel"
                    Size="2x2"
                    Row="2"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="Microsoft.Office.Desktop_8wekyb3d8bbwe!PowerPoint"
                    Size="2x2"
                    Row="2"
                    Column="2"/>
                <start:Tile
                    AppUserModelID="Microsoft.Office.OneNote_8wekyb3d8bbwe!microsoft.onenoteim"
                    Size="2x2"
                    Row="2"
                    Column="4"/>
            </start:Group>
            <start:Group
                LocalizedNameResourceTag="TileGrid_DefaultGroup2">
                <start:Tile
                    AppUserModelID="Microsoft.XboxApp_8wekyb3d8bbwe!Microsoft.XboxApp"
                    Size="2x2"
                    Row="0"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="Microsoft.ZuneVideo_8wekyb3d8bbwe!Microsoft.ZuneVideo"
                    Size="2x2"
                    Row="0"
                    Column="2"/>
                <start:Tile
                    AppUserModelID="Microsoft.Windows.Photos_8wekyb3d8bbwe!App"
                    Size="2x2"
                    Row="0"
                    Column="4"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:2!1"
                    Size="2x2"
                    Row="2"
                    Column="0"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:2!2"
                    Size="2x2"
                    Row="2"
                    Column="2"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:2!3"
                    Size="2x2"
                    Row="2"
                    Column="4"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:2!4"
                    Size="2x2"
                    Row="4"
                    Column="0"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:2!5"
                    Size="2x2"
                    Row="4"
                    Column="2"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:2!6"
                    Size="2x2"
                    Row="4"
                    Column="4"/>
            </start:Group>
            <start:Group
                LocalizedNameResourceTag="TileGrid_DefaultGroup3">
                <start:Tile
                    AppUserModelID="Microsoft.WindowsStore_8wekyb3d8bbwe!App"
                    Size="4x2"
                    Row="0"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="Microsoft.MicrosoftEdge_8wekyb3d8bbwe!MicrosoftEdge"
                    Size="2x2"
                    Row="0"
                    Column="4"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:1!1"
                    Size="2x2"
                    Row="2"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="microsoft.windowscommunicationsapps_8wekyb3d8bbwe!Microsoft.WindowsLive.Mail"
                    Size="2x2"
                    Row="2"
                    Column="2"/>
                <start:Tile
                    AppUserModelID="microsoft.windowscommunicationsapps_8wekyb3d8bbwe!Microsoft.WindowsLive.Calendar"
                    Size="2x2"
                    Row="2"
                    Column="4"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:1!2"
                    Size="2x2"
                    Row="4"
                    Column="0"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:1!3"
                    Size="2x2"
                    Row="4"
                    Column="2"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:1!4"
                    Size="1x1"
                    Row="4"
                    Column="4"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:1!5"
                    Size="1x1"
                    Row="4"
                    Column="5"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:1!6"
                    Size="1x1"
                    Row="5"
                    Column="4"/>
                <start:TargetedContentTile
                    TileID="T~DynamicLayouts:1!7"
                    Size="1x1"
                    Row="5"
                    Column="5"/>
            </start:Group>
        </StartLayout>

        <!-- 6 cell wide Desktop layout with targeted content tiles and Office 365 group -->
        <StartLayout
            GroupCellWidth="6"
            TargetedContentTilesEnabled="true"
            OfficeSKU="DesktopBridgeSubscription">
            <start:Group
                LocalizedNameResourceTag="TileGrid_Office365Group">
                <start:Tile
                    AppUserModelID="Microsoft.MicrosoftOfficeHub_8wekyb3d8bbwe!Microsoft.MicrosoftOfficeHub"
                    Size="4x2"
                    Row="0"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="Microsoft.Office.Desktop_8wekyb3d8bbwe!Outlook"
                    Size="2x2"
                    Row="0"
                    Column="4"/>
                <start:Tile
                    AppUserModelID="Microsoft.Office.Desktop_8wekyb3d8bbwe!Word"
                    Size="1x1"
                    Row="2"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="Microsoft.Office.Desktop_8wekyb3d8bbwe!Excel"
                    Size="1x1"
                    Row="2"
                    Column="1"/>
                <start:Tile
                    AppUserModelID="Microsoft.Office.Desktop_8wekyb3d8bbwe!PowerPoint"
                    Size="1x1"
                    Row="3"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="Microsoft.Office.OneNote_8wekyb3d8bbwe!microsoft.onenoteim"
                    Size="1x1"
                    Row="3"
                    Column="1"/>
                <start:DesktopApplicationTile
                    DesktopApplicationID="Microsoft.SkyDrive.Desktop"
                    Size="2x2"
                    Row="2"
                    Column="2"/>
